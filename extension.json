{
    "identifier": "sciencefidelity.dart",
    "name": "Dart",
    "organization": "Matt Cook",
    "description": "Dart Language Definition for Nova",
    "version": "0.2.1",
    "categories": ["clips", "completions", "languages"],
    "license": "MIT",
    "repository": "https://github.com/sciencefidelity/Dart.novaextension",
    "bugs": "https://github.com/sciencefidelity/Dart.novaextension/issues",
    "homepage": "https://mattcook.dev",
    "keywords": ["dart", "flutter", "angulardart"],

    "main": "main.js",

    "activationEvents": [
        "onLanguage:dart",
        "onWorkspaceContains:pubspec.yaml"
    ],
    "entitlements": {
        "process": true,
        "filesystem": "readonly"
    },

    "config": [
      {
          "key": "dart.language-server-path",
          "title": "Language Server Path",
          "type": "path",
          "placeholder": "/usr/local/flutter/bin/cache/dart-sdk/bin/snapshots"
      }
  ]
}
